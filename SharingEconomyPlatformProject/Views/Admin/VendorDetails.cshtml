<div class="container">
    <table class="table table-bordered table-hover ">
         <tr class="bg-success text-white">
            <th>FirstName</th>
            <th>LastName</th>
            <th>DOB</th>
            <th>Gender</th>
            <th>ContactNumber</th>
            <th>Address</th>
            <th>City</th>
            <th>State</th>
            <th>Zip</th>
            <th>Email</th>
            <th>Category</th>
            <th>VendorId</th>
            <th>Password</th>
            <th>ConfirmPassword</th>
            <th>Action</th>
            
           
           
        </tr>
        @{
            try
            {
                if(TempData["VendorM"]!=null)
                {
                    foreach (var x in (List<SharingEconomyPlatformProject.Models.Vendor>)TempData["VendorM"])
                    {
                                <tr>
                                    
                                    <td>@x.FirstName</td>
                                    <td>@x.LastName</td>
                                    <td>@x.DOB</td>
                                    <td>@x.Gender</td>
                                    <td>@x.ContactNumber</td>
                                    <td>@x.Address</td>
                                    <td>@x.City</td>
                                    <td>@x.State</td>
                                    <td>@x.zip</td>
                                    <td>@x.Email</td>
                                    <td>@x.Category</td>
                                    <td>@x.VendorId</td>
                                    <td>@x.Password</td>
                                    <td>@x.ConfirmPassword</td>
                                    <td>
                                    <a href="~/Admin/EditVendor/?id=@x.VendorId"class="btn btn-warning ">Update Data</a>
                                    <a href="~/Admin/DeleteVendor/?id=@x.VendorId "class="btn btn-danger "  >Remove Data</a>
                                   </td>
                                    
                                </tr>
                        
                    }
                }
            }
            catch
            {
                
            }
        }



    </table>

</div>